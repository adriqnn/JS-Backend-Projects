<div class="search-result">
    <div class="search-list">
        <div class="coin">
            <div class="coin-img">
                <img src="{{iamge}}">
            </div>
            <div class="coin-info">
                <h1>{{name}}</h1>
                <p><span>Price: </span>{{price}}</p>
                <p><span>Payment: </span>{{paymentMethod}}</p>
            </div>
            <a href="/model/{{_id}}/details" class="btn-details">Details</a>
        </div>
    </div>
</div>